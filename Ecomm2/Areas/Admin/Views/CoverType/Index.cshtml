@{
    ViewData["Title"] = "Category Page";
}
<div class="row">
    <div class="col-9">
        <h2 class="text-primary">Cover Type List</h2>
    </div>
    <div class="col-3">
        <a asp-action="Upsert" class="btn btn-info form-control">
            <i class="fas fa-plus"></i>&nbsp; Add New Cover Type
        </a>
    </div>
</div>
<div class="container rounded border p-2 m-2">
    <table id="tblData" class="table table-striped table-bordered table-hover">
        <thead class="bg-info">
            <tr>
                <th class="text-light">Edit</th>
                <th class="text-light">Name</th>
                <th class="text-light">Delete</th>
            </tr>
        </thead>
    </table>
</div>
@section Scripts  {
    <script src="~/js/coverType.js"></script>
}